{
  "name": "Great Extension",
  "version": "1.0",
  "description": "Build an Extension with Angular",
  "manifest_version": 2,
  "permissions": [
      "webRequest",
      "tabs",
      "activeTab",
      "alarms",
      "webNavigation",
      "storage",
      "https://www.nordnet.se/*"
  ],
  "background": {
      "scripts": [
          "background.js",
          "runtime.js",
          "inject.js"
      ]
  },
  "page_action": {
      "default_popup": "index.html"
  }
}
